{
    "name": "nextsb",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "lint": "eslint --ext='.ts,.tsx,.js,.jsx' ./src",
        "test": "jest",
        "clean": "rm -rf .next build/* docs/*",
        "build": "npm run clean && next build",
        "start": "next start",
        "export": "npm run build && next export -o docs",
        "serve": "http-server -p 3000 ./docs"
    },
    "dependencies": {
        "next": "10.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@testing-library/react": "^11.2.6",
        "@types/jest": "^26.0.23",
        "@types/node": "^15.0.2",
        "@types/react": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "babel-jest": "^26.6.3",
        "ecstatic": "^4.1.4",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "http-server": "^0.12.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "sass": "^1.32.12",
        "ts-jest": "^26.5.6",
        "typescript": "^4.2.4"
    }
}
