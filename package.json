    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/preset-typescript": "^7.13.0",
        "@testing-library/react": "^11.2.7",
        "@types/jest": "^26.0.23",
        "@types/marked": "^2.0.3",
        "@types/node": "^15.6.1",
        "@types/react": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "babel-jest": "^27.0.2",
        "ecstatic": "^4.1.4",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "http-server": "^0.12.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.0.3",
        "marked": "^2.0.7",
        "prettier": "^2.3.0",
        "sass": "^1.34.0",
        "ts-jest": "^27.0.2",
        "typescript": "^4.3.2"
    }
