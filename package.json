        "babel-jest": "^27.0.2",
        "eslint": "^7.28.0",
        "eslint-config-next": "^11.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest": "^24.3.6",
        "http-server": "^0.12.3",
        "identity-obj-proxy": "^3.0.0",
