{
    "name": "nextsb",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "lint": "eslint --ext='.ts,.tsx,.js,.jsx' ./src",
        "test": "jest",
        "clean": "rm -rf .next build/* docs/*",
        "build": "npm run clean && next build",
        "start": "next start",
        "export": "npm run build && next export -o docs",
        "serve": "http-server -p 3000 ./docs"
    },
    "dependencies": {
